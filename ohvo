		     <script type="text/javascript">
			     $("#cookiepopup").live("click", function(){
			    	 window.location.href = 'https://www.pacsun.com/account';
			    	 $('.cookiepopup').hide();
			 	});
			     
				jQuery(document).ready(function(){
					jQuery('.change-location-header a.flag').click(function(e){
						e.preventDefault();
					 	app.dialog.open("/international-shipping",' ');
					});

					var visited = getCookie("visited");
				    if (visited == "") {
				    	
				    	$('.cookiepopup').show();
				    	setCookie();
				    	
				    	setTimeout(function(){
				    		$('.cookiepopup').hide();
				    	}, 6000);
				        
				    } 
				    
				    function setCookie() {
				        document.cookie = 'visited=true';
				    }
				    
				    function getCookie(cname) {
				        var name = cname + "=";
				        var ca = document.cookie.split(';');
				        for(var i=0; i<ca.length; i++) {
				            var c = ca[i];
				            while (c.charAt(0)==' ') c = c.substring(1);
				            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
				        }
				        return "";
				    }
				})
				
			</script>
			
